{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "alvinhu/nova-child-select": "^1.0",
        "awesome-nova/dependent-filter": "^1.1",
        "bissolli/nova-phone-field": "0.0.4",
        "codebykyle/calculated-field": "^1.1",
        "dillingham/nova-ajax-select": "^1.0",
        "dillingham/nova-button": "^1.0",
        "dillingham/nova-detail-link": "^1.0",
        "fruitcake/laravel-cors": "2.0",
        "gobrightspot/nova-detached-actions": "^1.1",
        "gregoriohc/laravel-nova-theme-responsive": "0.8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "kirschbaum-development/nova-comments": "^1.0",
        "laraning/nova-time-field": "^1.2",
        "laravel/framework": "^8.54",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "~1.0",
        "m-a-k-o/nova-custom-table-card": "3.3",
        "maatwebsite/laravel-nova-excel": "1.2.3",
        "optimistdigital/nova-detached-filters": "^1.1",
        "optimistdigital/nova-input-filter": "^1.1",
        "pdmfc/nova-action-button": "^0.7.4",
        "sendgrid/sendgrid": "^7.10",
        "simonhamp/laravel-nova-csv-import": "0.3",
        "sparclex/nova-import-card": "*",
        "spatie/laravel-permission": "^5.1",
        "ugduck/admin-dashboard": "*",
        "ugduck/infinitycomm": "*",
        "ugduck/infoclient": "*",
        "ugduck/myprofile": "*",
        "ugduck/reset-password": "*",
        "wehaa/custom-links": "0.1.1",
        "yassi/nova-nested-form": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.4",
        "laravel/sail": "^1.10",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/general.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/Myprofile"
        },
        {
            "type": "path",
            "url": "./nova-components/Infoclient"
        },
        {
            "type": "path",
            "url": "./nova-components/ResetPassword"
        },
        {
            "type": "path",
            "url": "./nova-components/Infinitycomm"
        },
        {
            "type": "path",
            "url": "./nova-components/AdminDashboard"
        },
        {
            "type": "path",
            "url": "./nova-components/AdminDashboard"
        }
    ]
}
