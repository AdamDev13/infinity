{
    "name": "ugduck/reset-password",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ugduck\\ResetPassword\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ugduck\\ResetPassword\\ToolServiceProvider"
            ]
        }
    },
    "config": {
